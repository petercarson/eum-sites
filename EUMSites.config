<settings>
    <common>
        <sharePointOnline>true</sharePointOnline>
        <useMixedModeAuthentication>false</useMixedModeAuthentication>
        <siteLists>
            <siteListName>Sites</siteListName>
        </siteLists>
    </common>
    <environments>
      <environment>
        <id>1</id>
        <name>Dev</name>
        <webApp>
          <adminSiteURL>https://keithtuomi-admin.sharepoint.com</adminSiteURL>
          <url>https://keithtuomi.sharepoint.com</url>
          <sitesListSiteCollectionPath>/sites/ENV</sitesListSiteCollectionPath>
          <managedCredentials>keithtuomi.sharepoint.com</managedCredentials>
        </webApp>
      </environment>
      <environment>
        <id>2</id>
        <name>UAT</name>
        <webApp>
          <adminSiteURL>https://envisionit-admin.sharepoint.com</adminSiteURL>
          <url>https://envisionit.sharepoint.com</url>
          <sitesListSiteCollectionPath>/sites/eumsites</sitesListSiteCollectionPath>
          <managedCredentials>envisionit.sharepoint.com</managedCredentials>
        </webApp>
      </environment>
    </environments>
</settings>